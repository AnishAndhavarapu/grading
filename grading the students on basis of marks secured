#include <stdio.h>
int main()
{
	int marks=0;
	printf("enter marks secured by the student");
	scanf("%s",marks);
	if(marks>=85 && marks<=100)
	{
		printf(" A-grade");
	
	}
	else if(marks>=70 && marks<85)
	{
		printf("B-grade");
	}
	else if(marks>=55 && marks<70)
	{
		printf("C-grade");
	}
	else if(marks>=40 && marks<55)
	{
		printf("D-grade");
	}
	else 
	{
		printf("F-grade");
	}
	return 0;
}
